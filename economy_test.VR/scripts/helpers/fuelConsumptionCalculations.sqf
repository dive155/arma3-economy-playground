PDR_fuelConsumptionMultiplier = 0.65;

fnc_getEconomyFuelConsumption = {
	params["_vehicle"];
	private _class = typeOf _vehicle;
	private _value = PDR_allFuelCoefficients getOrDefault [_class, -1];
	
	if (_value != -1) then {
		_value = _value * PDR_fuelConsumptionMultiplier;
	};
	
	_value
};

PDR_allFuelCoefficients = createHashMap;

// Create base map directly from array
private _baseMap = createHashMapFromArray PDR_baseFuelCoefficients;

// Now, assign coefficients to all classes (base and aliases)
{
	private _baseClass = _x select 0;
	private _aliases = _x; // Includes base class and all aliases
	
	// Get the coefficient for this base class
	private _coef = _baseMap get _baseClass;
	
	// Assign the coefficient to all class names in this alias list
	{
		PDR_allFuelCoefficients set [_x, _coef];
	} forEach _aliases;
} forEach PDR_classesAliases;
